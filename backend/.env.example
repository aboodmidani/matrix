# Backend Environment Configuration
# Copy this file to .env and set your values

# Server Configuration
HOST=0.0.0.0
PORT=8000
ENVIRONMENT=production

# CORS Configuration
# Comma-separated list of allowed origins
CORS_ORIGINS=https://matrix-audit.netlify.app,http://localhost:5173,http://localhost:3000

# Security
DEBUG=false
LOG_LEVEL=INFO

# Tool Configuration
# Timeout for scanning tools in seconds
SCAN_TIMEOUT=120